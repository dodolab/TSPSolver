{
	"compilerOptions": {
		"target": "ES6",
		"module": "commonjs",
		"moduleResolution": "node",
		"esModuleInterop": true,
		"resolveJsonModule": true, /* allows importing modules with a JSON extension */
		"downlevelIteration": true /* enables transpiling to an older JS version */
	},
	"include": ["src/"],
}
